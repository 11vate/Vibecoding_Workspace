{
  "name": "ai-sprite-generator",
  "version": "1.0.0",
  "description": "Intelligent AI Sprite Generator - Production-ready game asset generation",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "ai-sprite-gen": "./dist/cli.js"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "build:check": "tsc",
    "dev": "tsx src/cli.ts",
    "start": "node dist/cli.js",
    "dev:api": "tsx src/api/server.ts",
    "dev:ui": "cd ui && vite",
    "build:ui": "cd ui && vite build",
    "test": "vitest",
    "test:watch": "vitest --watch"
  },
  "keywords": [
    "sprite",
    "generator",
    "ai",
    "stable-diffusion",
    "game-assets",
    "pixel-art"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "sharp": "^0.33.2",
    "zod": "^3.22.4",
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/better-sqlite3": "^7.6.8",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

